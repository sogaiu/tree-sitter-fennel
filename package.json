{
  "name": "tree-sitter-fennel",
  "version": "0.0.2",
  "description": "Fennel grammar for tree-sitter",
  "main": "bindings/node",
  "scripts": {
    "build": "npx tree-sitter generate && npx node-gyp build",
    "fresh-build": "npx tree-sitter generate && npx node-gyp configure && npx node-gyp rebuild"
  },
  "author": "",
  "license": "",
  "dependencies": {
    "nan": "2.14.2"
  },
  "devDependencies": {
    "tree-sitter-cli": "0.19.4"
  },
  "tree-sitter": [
    {
      "scope": "source.fennel",
      "file-types": [
        "fnl"
      ]
    }
  ]
}
